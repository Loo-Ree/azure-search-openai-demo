{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "blobs": {
        "maxDegreeOfParallelism": 1,
        "poisonBlobThreshold": 2
    }
 },
  "functionTimeout": "00:59:00",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.15.0, 5.0.0)"
  }
}